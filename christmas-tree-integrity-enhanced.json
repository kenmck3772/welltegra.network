  "test_procedures": {
    "proc_001": {
      "id": "proc_001",
      "name": "Christmas Tree Annual Pressure Test",
      "procedure_number": "OP-CT-002",
      "frequency": "Annual",
      "test_type": "Pressure Test",
      "description": "Full pressure test of all tree valves to verify integrity",
      "steps": [
        "Isolate well and bleed down pressure",
        "Close all valves",
        "Pressure up cavity between UMV and LMV to 8,500 PSI",
        "Hold for 30 minutes and monitor for pressure drop",
        "Bleed down and inspect for leaks",
        "Repeat for Production Wing and Kill Wing valves",
        "Document results and witness signatures"
      ],
      "acceptance_criteria": "No pressure drop >2% over 30 minutes",
      "required_equipment": [
        "High pressure pump",
        "Calibrated pressure gauge",
        "Bleed-off equipment"
      ],
      "personnel_required": 3,
      "estimated_duration": "8 hours",
      "regulatory_reference": "OGUK Well Integrity Guidelines Section 4.2"
    },
    "proc_002": {
      "id": "proc_002",
      "name": "Monthly Valve Function Test",
      "procedure_number": "OP-CT-001",
      "frequency": "Monthly",
      "test_type": "Function Test",
      "description": "Cycle all valves to verify operation",
      "steps": [
        "Verify well is stable and isolated",
        "Slowly stroke each valve from fully open to fully closed",
        "Verify position indicators match valve position",
        "Check for external leaks during stroking",
        "Lubricate valve stems as required",
        "Return valves to operational positions",
        "Document results"
      ],
      "acceptance_criteria": "All valves stroke smoothly without binding, no external leaks",
      "required_equipment": [
        "Torque wrench",
        "Valve grease",
        "Inspection mirror"
      ],
      "personnel_required": 2,
      "estimated_duration": "4 hours"
    },
    "proc_003": {
      "id": "proc_003",
      "name": "DHSV Function Test",
      "procedure_number": "OP-DHSV-001",
      "frequency": "Monthly",
      "test_type": "Function Test",
      "description": "Test downhole safety valve closure on demand",
      "steps": [
        "Record current wellhead pressure",
        "Close surface master valves",
        "Bleed hydraulic control line pressure",
        "Verify DHSV closes (pressure should drop)",
        "Re-pressurize control line",
        "Verify DHSV opens (pressure should increase)",
        "Return system to normal operation",
        "Document closure/opening times"
      ],
      "acceptance_criteria": "Valve closes in <60 seconds, opens in <30 seconds",
      "required_equipment": [
        "Hydraulic pump",
        "Pressure recorder"
      ],
      "personnel_required": 2,
      "estimated_duration": "2 hours",
      "regulatory_reference": "API RP 14B Section 5.3"
    },
    "proc_004": {
      "id": "proc_004",
      "name": "Annulus Bleed-Down Test",
      "procedure_number": "OP-WH-003",
      "frequency": "Quarterly",
      "test_type": "Leak Test",
      "description": "Test tubing and casing integrity via annulus monitoring",
      "steps": [
        "Record initial A-annulus and B-annulus pressures",
        "Bleed down A-annulus to zero pressure",
        "Monitor for pressure buildup over 24 hours",
        "Calculate pressure buildup rate",
        "Repeat for B-annulus",
        "Analyze results for tubing/casing leaks",
        "Document findings"
      ],
      "acceptance_criteria": "Pressure buildup <50 PSI/24hr",
      "required_equipment": [
        "Calibrated pressure gauges",
        "Bleed-off valves",
        "Data logger"
      ],
      "personnel_required": 2,
      "estimated_duration": "48 hours (24hr monitoring)",
      "regulatory_reference": "NORSOK D-010 Section 8.4"
    },
    "proc_005": {
      "id": "proc_005",
      "name": "ESD System Full Function Test",
      "procedure_number": "OP-ESD-001",
      "frequency": "Quarterly",
      "test_type": "System Test",
      "description": "Test complete emergency shutdown sequence",
      "steps": [
        "Notify operations and obtain isolation permit",
        "Shut in well on production wing valve",
        "Initiate ESD from control panel",
        "Verify ESDV closes",
        "Verify master valves close (if auto)",
        "Verify alarms activate",
        "Reset ESD system",
        "Verify all valves return to normal",
        "Document closure times and sequence"
      ],
      "acceptance_criteria": "All valves close in required sequence, <10 seconds total",
      "required_equipment": [
        "ESD panel",
        "Stopwatch",
        "Witness personnel"
      ],
      "personnel_required": 4,
      "estimated_duration": "4 hours",
      "regulatory_reference": "IEC 61511 Section 11.8"
    }
  },
